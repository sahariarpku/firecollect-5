# Supabase Configuration
# Get these values from your Supabase project settings (https://supabase.com/dashboard/project/_/settings)
NEXT_PUBLIC_SUPABASE_URL=your_project_url  # e.g., https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key  # From Project Settings > API > Project API keys
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key  # From Project Settings > API > Project API keys
SUPABASE_DB_PASSWORD=your_database_password  # From Project Settings > Database > Database Password
SUPABASE_PROJECT_ID=your_project_id  # The reference ID of your project (found in Project Settings > General)

# API Configuration
NEXT_PUBLIC_FIRECRAWL_API_URL=https://api.firecrawl.dev

# Instructions:
# 1. Create a new project on Supabase (https://supabase.com/dashboard)
# 2. Copy the values from your project settings
# 3. Rename this file to .env.local
# 4. Replace the placeholder values with your actual credentials

VITE_SUPABASE_URL=your_project_url
VITE_SUPABASE_ANON_KEY=your_anon_key
VITE_SUPABASE_PROJECT_ID=your_project_id 